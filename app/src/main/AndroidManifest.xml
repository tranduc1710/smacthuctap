<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tranduc.smacthuctap">

    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.Canvas.CanvasActivity"></activity>
        <activity android:name=".view.DayNine.DayNine" />
        <activity android:name=".view.DayEight.DayEight" />

        <service
            android:name=".view.DaySix.PlayMusicService"
            android:enabled="true"
            android:exported="true" />

        <activity android:name=".view.DaySix.DaySix" />
        <activity android:name=".view.DayFive.DayFive" />
        <activity android:name=".view.DayFour.DayFour" />
        <activity android:name=".view.DayThree.DayThree" />
        <activity android:name=".view.DayTwo.DayTwo" />
        <activity android:name=".view.MainActivity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".view.DayOne.DayOne" />
    </application>

</manifest>